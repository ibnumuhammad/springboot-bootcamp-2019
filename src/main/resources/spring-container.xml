<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean id="dimas" class="com.tabeldata.bootcamp.belajarspringboot.Mahasiswa">
        <property name="nama" value="Dimas maryanto"/>
        <property name="semester" value="1"/>
        <property name="kelas" ref="si"/>
    </bean>
    
    <bean id="si" class="com.tabeldata.bootcamp.belajarspringboot.Kelas">
        <property name="nama" value="Sistem informasi"/>
    </bean>
</beans>